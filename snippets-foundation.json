            "fd:alert": "<div data-alert class=\"alert-box [success warning info secondary alert radius round]\">This is a success alert with a radius.<a href=\"#\" class=\"close\">&times;</a></div>",
            "fdng:alert": "<alert ng-repeat=\"alert in alerts\" type=\"alert.type\" close=\"closeAlert($index)\">{{alert.msg}}</alert>",
            "fd:topbar": "<!--<div class=\"contain-to-grid fixed sticky\">--><nav class=\"top-bar\" data-topbar><ul class=\"title-area\"><li class=\"name\"><h1><a href=\"#\">My Site</a></h1> </li><li class=\"toggle-topbar menu-icon\"><a href=\"#\"><span><!--Menu--></span></a></li></ul><section class=\"top-bar-section\"><!-- Right Nav Section --><ul class=\"right\"><li class=\"active\"><a href=\"#\">Active</a></li><li class=\"has-dropdown\"> <a href=\"#\">Dropdown</a> <ul class=\"dropdown\"><li><a href=\"#\">First link in dropdown</a></li></ul></li></ul><!-- Left Nav Section --><ul class=\"left\"><li><a href=\"#\">Left</a></li></ul></section></nav><!--</div>-->",
            "fdng:topbar": "<!--<div class=\"contain-to-grid fixed sticky\">--><top-bar><ul class=\"title-area\"><li class=\"name\"><h1><a href=\"#\">My Site</a></h1></li><li toggle-top-bar class=\"menu-icon\"><a href=\"#\"><span><!--Menu--></span></a></li></ul><top-bar-section><!-- Right Nav Section --><ul class=\"right\"><li class=\"active\"><a href=\"#\">Right Button Active</a></li><li has-dropdown><a href=\"#\">Dropdown</a><ul top-bar-dropdown><li><a href=\"#\">First link in dropdown</a></li></ul></li></ul><!-- Left Nav Section --><ul class=\"left\"><li><a href=\"#\">Left</a></li></ul></top-bar-section></top-bar><!--</div>-->",
		"fd:offcanvas": "<div class=\"off-canvas-wrap\" data-offcanvas> <div class=\"inner-wrap\"> <nav class=\"tab-bar\"> <section class=\"left-small\"> <a class=\"left-off-canvas-toggle menu-icon\" href=\"#\"><span></span></a> </section> <section class=\"middle tab-bar-section\"> <h1 class=\"title\">My Site</h1> </section> <section class=\"right-small\"> <a class=\"right-off-canvas-toggle menu-icon\" href=\"#\"><span></span></a> </section> </nav> <aside class=\"left-off-canvas-menu\"> <ul class=\"off-canvas-list\"> <li><label>List Heading</label></li> <li><a href=\"#\">Item 1</a></li> <li><a href=\"#\">Item 2</a></li> </ul> </aside> <aside class=\"right-off-canvas-menu\"> <ul class=\"off-canvas-list\"> <li><label>List Heading</label></li> <li><a href=\"#\">Item 1</a></li> <li><a href=\"#\">Item 2</a></li> </ul> </aside> <section class=\"main-section\"> <!-- content goes here --> </section> <a class=\"exit-off-canvas\"></a> </div> </div>",
            "fdng:offcanvas": "<div class=\"off-canvas-wrap\"><div class=\"inner-wrap\"><nav class=\"tab-bar\"><section class=\"left-small\"><a class=\"left-off-canvas-toggle menu-icon\" ><span></span></a></section><section class=\"middle tab-bar-section\"><h1 class=\"title\">OffCanvas</h1></section><section class=\"right-small\"><a class=\"right-off-canvas-toggle menu-icon\" ><span></span></a></section></nav><aside class=\"left-off-canvas-menu\"><ul class=\"off-canvas-list\"><li><a href=\"#\">Left Sidebar</a></li></ul></aside><aside class=\"right-off-canvas-menu\"><ul class=\"off-canvas-list\"><li><a href=\"#\">Right Sidebar</a></li></ul></aside><section class=\"main-section\"><div class=\"small-12 columns\"><h1>How to use</h1><p>Just use the standard layout for an offcanvas page as documented in the <a href=\"http://foundation.zurb.com/docs/components/offcanvas.html\">foundation docs</a></p><p>As long as you include mm.foundation.offcanvas it should simply work</p></div></section><a class=\"exit-off-canvas\"></a></div></div>",
            "fd:iconbar": "<!-- .fa requires font awesome --><div class=\"icon-bar [one-up ... six-up] [vertical]\"><a class=\"item\"> <i class=\"fa fa-cubes\"></i><label>One</label></a><a class=\"item\"> <i class=\"fa fa-database\"></i><label>Two</label></a></div>",
            "fd:sidenav": "<ul class=\"side-nav\"> <li class=\"active\"><a href=\"#\">Link 1</a></li> <li><a href=\"#\">Link 2</a></li> <li class=\"divider\"></li> <li><a href=\"#\">Link 3</a></li> <li><a href=\"#\">Link 4</a></li> </ul>",
            "fd:magellan": "<div data-magellan-expedition=\"fixed\"> <dl class=\"sub-nav\"> <dd data-magellan-arrival=\"anchor1\"><a href=\"#anchor1\">Link to Anchor 1</a></dd> <dd data-magellan-arrival=\"anchor2\"><a href=\"#anchor2\">Link to Anchor 2</a></dd> </dl> </div>",
            "fd:subnav": "  <!-- useful for filter or page state --><dl class=\"sub-nav\"> <dt>Filter:</dt> <dd class=\"active\"><a href=\"#\">All</a></dd> <dd><a href=\"#\">Active</a></dd> <dd><a href=\"#\">Pending</a></dd> <dd><a href=\"#\">Suspended</a></dd> </dl>",
            "fd:breadcrumbs": "<nav class=\"breadcrumbs\"><a href=\"#\">Home</a><a href=\"#\">Features</a><a class=\"unavailable\" href=\"#\">Gene Splicing</a><a class=\"current\" href=\"#\">Cloning</a></nav>",
            "fd:pagination": "<!--<div class=\"pagination-centered\">--><ul class=\"pagination [left right]\"> <li class=\"arrow unavailable\"><a href=\"\">&laquo;</a></li> <li class=\"current\"><a href=\"\">1</a></li> <li><a href=\"\">2</a></li> <li><a href=\"\">3</a></li> <li><a href=\"\">4</a></li> <li class=\"unavailable\"><a href=\"\">&hellip;</a></li> <li><a href=\"\">12</a></li> <li><a href=\"\">13</a></li> <li class=\"arrow\"><a href=\"\">&raquo;</a></li></ul><!--</div>-->",
            "fdng:pagination": "<!-- see docs for options --><pagination total-items=\"totalItems\" page=\"currentPage\"></pagination>",
            "fd:imglink": "<a class=\"th radius\" href=\"\"><img src=\"\"></a>",
            "fd:clearing": "<ul class=\"clearing-thumbs small-block-grid-4\" data-clearing><li><a href=\"path/to/your/img\"><img data-caption=\"caption here...\" src=\"path/to/your/img-th\"></a></li></ul>",
            "fd:clearing:feature": "<ul class=\"clearing-thumbs clearing-feature\" data-clearing><li class=\"clearing-featured-img\"><a href=\"path/to/your/img\"><img src=\"path/to/your/img-th\"></a></li><li><a href=\"path/to/your/img\"><img src=\"path/to/your/img-th\"></a></li></ul>",
            "fd:input:postfix": "<div class=\"row collapse\"><label>Label</label><div class=\"small-9 columns\"><input type=\"text\" placeholder=\"\" /></div><div class=\"small-3 columns\"><span class=\"postfix\"><i class=\"fa fa-search\"></i></span></div></div>",
            "fd:input:prefix": "<div class=\"row collapse\"><div class=\"small-3 large-2 columns\"><span class=\"prefix\">http://</span></div><div class=\"small-9 large-10 columns\"><input type=\"text\" placeholder=\"\"></div></div>",
            "fd:switch": "<div class=\"switch [tiny small large radius round]\"><input id=\"example\" type=\"checkbox\"><label for=\"example\"></label></div>",
            "fd:switch:radio": "<div class=\"switch [tiny small large radius round]\"><input id=\"example1\" type=\"radio\" checked name=\"testGroup\"><label for=\"example1\"></label></div><div class=\"switch\"><input id=\"example2\" type=\"radio\" name=\"testGroup\"><label for=\"example2\"></label></div>",
            "fd:rangeslider": "<div class=\"range-slider\" data-slider> <span class=\"range-slider-handle\"></span> <span class=\"range-slider-active-segment\"></span> <input type=\"hidden\"></div>",
            "fd:rangeslider:vertical": "<div class=\"range-slider vertical-range\" data-slider data-options=\"vertical: true;\"> <span class=\"range-slider-handle\"></span> <span class=\"range-slider-active-segment\"></span><input type=\"hidden\"></div>",
            "fd:rangeslider:label": "<div class=\"row\"><div class=\"small-10 medium-11 columns\"><div class=\"range-slider\" data-slider data-options=\"display_selector: #sliderOutput3;\"><span class=\"range-slider-handle\"></span><span class=\"range-slider-active-segment\"></span></div></div><div class=\"small-2 medium-1 columns\"><span id=\"sliderOutput3\"></span></div></div>",
            "fd:form:login": "<!-- data-abide performs javascript form validation --><form data-abide><div class=\"email-field\"><label>Email<small>required</small><input type=\"email\" required></label><small class=\"error\">An email address is required.</small></div><div class=\"password-field\"><label>Password<small>required</small><input type=\"password\" id=\"password\" required></label><small class=\"error\">Your password must match the requirements</small></div><button type=\"submit\">Submit</button></form>",
            "fd:form:register": "<!-- data-abide performs javascript form validation --><form data-abide><div class=\"name-field\"><label>Your name <small>required</small><input type=\"text\" required pattern=\"alpha\"></label><small class=\"error\">Name is required and must be a string.</small></div><div class=\"email-field\"><label>Email<small>required</small><input type=\"email\" required></label><small class=\"error\">An email address is required.</small></div><div class=\"password-field\"><label>Password<small>required</small><input type=\"password\" id=\"password\" required></label><small class=\"error\">Your password must match the requirements</small></div><div class=\"password-confirmation-field\"><label>Confirm Password <small>required</small><input type=\"password\" required data-equalto=\"password\"></label><small class=\"error\">The password did not match</small></div><button type=\"submit\">Submit</button></form>",
            "fd:button:group": "<!--<div class=\"button-bar\">--><ul class=\"button-group [radius round even-3]\"><li><a href=\"#\" class=\"[tiny small large] button [alert success secondary] [disabled]\">Button 1</a></li><li><a href=\"#\" class=\"[tiny small large] button [alert success secondary] [disabled]\">Button 2</a></li><li><a href=\"#\" class=\"[tiny small large] button [alert success secondary] [disabled]\">Button 3</a></li></ul><!--</div>-->",
            "fd:button:split": "<a href=\"#\" class=\"button split\">Split Button <span data-dropdown=\"drop1\"></span></a><br> <ul id=\"drop1\" class=\"f-dropdown\" data-dropdown-content> <li><a href=\"#\">Link 1</a></li> <li><a href=\"#\">Link 2</a></li></ul>",
            "fdng:button:split": "<a class=\"button split\">Split Button <span dropdown-toggle=\"drop1\"></span></a><br><ul id=\"drop1\" class=\"f-dropdown\"><li><a>Link 1</a></li><li><a>Link 2</a></li></ul>",
            "fd:button:dropdown": "<a href=\"#\" data-dropdown=\"drop1\" class=\"button dropdown\">Dropdown Button</a><br> <ul id=\"drop1\" data-dropdown-content class=\"f-dropdown\"> <li><a href=\"#\">Link 1</a></li> <li><a href=\"#\">Link 2</a></li></ul>",
            "fdng:button:dropdown": "<a href=\"#\" dropdown-toggle=\"drop1\" class=\"button dropdown\">Dropdown Button</a><br><ul id=\"drop1\"  class=\"f-dropdown\"><li><a href=\"#\">Link 1</a></li><li><a href=\"#\">Link 2</a></li></ul>",
            "fd:modal": "<a href=\"#\" data-reveal-id=\"myModal\">Click Me For A Modal</a><div id=\"myModal\" class=\"reveal-modal [tiny small medium large xlarge full]\" data-reveal><h2>Awesome. I have it.</h2><p class=\"lead\">Your couch. It is mine.</p><p>Im a cool paragraph that lives inside of an even cooler modal. Wins</p><a class=\"close-reveal-modal\">&#215;</a></div>",
            "fdng:modal": "<!-- see docs for controller code  https://madmimi.github.io/angular-foundation/ --><script type=\"text/ng-template\" id=\"myModalContent.html\"><h3>I 'm a modal!</h3><ul><li ng-repeat=\"item in items\"><a ng-click=\"selected.item = item\">{{ item }}</a></li></ul><p>Selected: <b>{{ selected.item }}</b></p><button class=\"button\" ng-click=\"ok()\">OK</button><a class=\"close-reveal-modal\" ng-click=\"cancel()\">&#215;</a></script><button class=\"button\" ng-click=\"open()\">Open me!</button>",
            "fd:panel": "<div class=\"panel [callout radius]\"><h5>This is a callout panel.</h5><p>It's a little ostentatious, but useful for important content.</p></div>",
            "fd:tooltip": "<span data-tooltip data-options=\"[disable_for_touch:true;show_on:small]\" class=\"has-tip\" title=\"Tooltips are awesome, you should totally use them!\">extended information</span>",
            "fd:joyride": "<!-- ol or ul, insert at end of body, match data-id with element id --><ul class=\"joyride-list\" data-joyride><li data-id=\"firstStop\" data-text=\"Next\" data-options=\"tip_location: top\"><p>Hello and welcome to the Joyride documentation page.</p></li><li data-id=\"numero1\" data-class=\"custom so-awesome\" data-text=\"Next\"><h4>Stop #1</h4><p>You can control all the details for you tour stop. Any valid HTML will work inside of Joyride.</p></li><li data-id=\"numero2\" data-button=\"Next\" data-options=\"tip_location:top;tip_animation:fade\"><h4>Stop #2</h4><p>Get the details right by styling Joyride with a custom stylesheet!</p></li><li data-button=\"End\"><h4>Stop #3</h4><p>It works as a modal too!</p></li></ul>",
            "fd:pricingtable": "<ul class=\"pricing-table\"><li class=\"title\">Standard</li><li class=\"price\">$99.99</li><li class=\"description\">An awesome description</li><li class=\"bullet-item\">1 Database</li><li class=\"bullet-item\">5GB Storage</li><li class=\"bullet-item\">20 Users</li><li class=\"cta-button\"><a class=\"button\" href=\"#\">Buy Now</a></li></ul>",
            "fd:progressbar": "<div class=\"progress [small-# large-#] [secondary alert success] [radius round]\"><span class=\"meter\" [style=\"width: [1 - 100]%\"]></span></div>",
            "fd:table": "<table><thead><tr><th>Table Header</th><th>Table Header</th></tr></thead><tbody><tr><td>Content Goes Here</td><td>Content Goes Here</td></tr><tr><td>Content Goes Here</td><td>Content Goes Here</td></tr></tbody></table>",
            "fd:accordian": "<dl class=\"accordion\" data-accordion><dd class=\"accordion-navigation\"><a href=\"#panel1\">Accordion 2</a><div id=\"panel1\" class=\"content\">Content area.  You can put text or HTML here.</div></dd><dd class=\"accordion-navigation\"><a href=\"#panel2\">Accordion 3</a><div id=\"panel2\" class=\"content\">Content area.</div></dd></dl>",
            "fdng:accordian": "<accordion close-others=\"true\"><accordion-group heading=\"Heading attribute\" is-open=\"true\">Content content</accordion-group><accordion-group><accordion-heading>Heading tag</accordion-heading>content content</accordion-group></accordion>",
            "fd:tabs": "<!-- add .vertical to ul.tabs and div.tabs-content for vertical tabs --><ul class=\"tabs\" data-tab><li class=\"tab-title active\"><a href=\"#panel2-1\">Tab 1</a></li><li class=\"tab-title\"><a href=\"#panel2-2\">Tab 2</a></li><li class=\"tab-title\"><a href=\"#panel2-3\">Tab 3</a></li></ul><div class=\"tabs-content\"><div class=\"content active\" id=\"panel2-1\"><p>First panel content goes here...</p></div><div class=\"content\" id=\"panel2-2\"><p>Second panel content goes here...</p></div><div class=\"content\" id=\"panel2-3\"><p>Third panel content goes here...</p></div></div>",
            "fdng:tabs": "<tabset><tab heading=\"Heading Attr\" active=\"true\">Content content</tab><tab select=\"fn()\"><tab-heading>Heading tag</tab-heading>Content content</tab></tabset>",
